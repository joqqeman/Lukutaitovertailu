
public class Maa {
    
    private String nimi;
    private int vuosi;
    private String sukupuoli;
    private int lukutaito;
    
    public Maa(String nimi, int vuosi, String sukupuoli, int lukutaito) {
        this.nimi = nimi;
        this.vuosi = vuosi;
        this.sukupuoli = sukupuoli;
        this.lukutaito = lukutaito;
    }
    
    public int getLukutaito() {
        return lukutaito;
    }
    
    public String toString() {
        return this.nimi + " (" + this.vuosi + "), " + this.sukupuoli + ", " + this.lukutaito;
    }
}
